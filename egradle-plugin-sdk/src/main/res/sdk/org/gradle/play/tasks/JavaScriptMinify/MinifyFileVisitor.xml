<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type language="gradle" name="org.gradle.play.tasks.JavaScriptMinify.MinifyFileVisitor" version="3.0">
    <description> Copies each file in the source set to the output directory and gathers relative files for compilation

</description>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.api.file.CopySpec" name="copySpec"/>
    </method>
    <method name="visitDir" returnType="void">
        <parameter type="org.gradle.api.file.FileVisitDetails" name="dirDetails"/>
    </method>
    <method name="visitFile" returnType="void">
        <parameter type="org.gradle.api.file.FileVisitDetails" name="fileDetails"/>
    </method>
    <interface name="org.gradle.api.file.FileVisitor"/>
</type>
