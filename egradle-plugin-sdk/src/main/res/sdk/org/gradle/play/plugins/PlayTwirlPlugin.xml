<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type language="gradle" name="org.gradle.play.plugins.PlayTwirlPlugin" version="3.0" superType="org.gradle.model.RuleSource">
    <description> 
 Plugin for compiling Twirl sources in a Play application. 
</description>
    <method name="addPlayJavaDependencyIfNeeded" returnType="void">
        <parameter type="org.gradle.model.ModelMap" name="binaries"/>
        <parameter type="org.gradle.play.plugins.PlayPluginConfigurations" name="configurations"/>
        <parameter type="org.gradle.platform.base.internal.PlatformResolvers" name="platforms"/>
    </method>
    <method name="createGeneratedScalaSourceSets" returnType="void">
        <parameter type="org.gradle.model.ModelMap" name="binaries"/>
        <parameter type="org.gradle.api.internal.file.SourceDirectorySetFactory" name="sourceDirectorySetFactory"/>
    </method>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.play.internal.PlayApplicationBinarySpecInternal" name="playApplicationBinarySpec"/>
    </method>
    <method name="hasTwirlSourceSetsWithJavaImports" returnType="boolean">
        <parameter type="org.gradle.play.PlayApplicationSpec" name="playApplicationSpec"/>
    </method>
    <method name="isSatisfiedBy" returnType="boolean">
        <parameter type="org.gradle.language.twirl.TwirlSourceSet" name="twirlSourceSet"/>
    </method>
    <method name="registerLanguageTransform" returnType="void">
        <parameter type="org.gradle.language.base.internal.registry.LanguageTransformContainer" name="languages"/>
    </method>
    <method name="registerTwirlLanguageType" returnType="void">
        <parameter type="org.gradle.platform.base.TypeBuilder" name="builder"/>
    </method>
    <property name="satisfiedBy" type="boolean"/>
</type>
