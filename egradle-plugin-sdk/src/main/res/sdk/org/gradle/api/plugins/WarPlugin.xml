<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type interface="false" language="gradle" name="org.gradle.api.plugins.WarPlugin" version="3.0">
    <description> 
 &lt;p&gt;A &lt;a href='type://Plugin'&gt;Plugin&lt;/a&gt; which extends the &lt;a href='type://JavaPlugin'&gt;JavaPlugin&lt;/a&gt; to add tasks which assemble a web application into a WAR 
 file.&lt;/p&gt; 
</description>
    <method name="apply" returnType="void">
        <parameter type="org.gradle.api.Project" name="project"/>
    </method>
    <method name="call" returnType="java.lang.Object"/>
    <method name="configureComponent" returnType="void">
        <parameter type="org.gradle.api.Project" name="project"/>
        <parameter type="org.gradle.api.artifacts.PublishArtifact" name="warArtifact"/>
    </method>
    <method name="configureConfigurations" returnType="void">
        <parameter type="org.gradle.api.artifacts.ConfigurationContainer" name="configurationContainer"/>
    </method>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.api.tasks.bundling.War" name="task"/>
    </method>
</type>
