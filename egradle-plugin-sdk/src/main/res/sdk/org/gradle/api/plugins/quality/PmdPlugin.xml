<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type language="gradle" name="org.gradle.api.plugins.quality.PmdPlugin" version="3.0" superType="org.gradle.api.plugins.quality.internal.AbstractCodeQualityPlugin">
    <description> A plugin for the &lt;a href="http://pmd.sourceforge.net/"&gt;PMD&lt;/a&gt; source code analyzer.
 &lt;p&gt;
 Declares a &lt;tt&gt;pmd&lt;/tt&gt; configuration which needs to be configured with the PMD library to be used.
 &lt;p&gt;
 For each source set that is to be analyzed, a &lt;a href='type://Pmd'&gt;Pmd&lt;/a&gt; task is created and configured to analyze all Java code.
 &lt;p&gt;
 All PMD tasks (including user-defined ones) are added to the &lt;tt&gt;check&lt;/tt&gt; lifecycle task.

 (see &lt;a href='type://PmdExtension'&gt;PmdExtension&lt;/a&gt;)
 (see &lt;a href='type://Pmd'&gt;Pmd&lt;/a&gt;)

</description>
    <method name="call" returnType="java.util.List"/>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.api.artifacts.DependencySet" name="dependencies"/>
    </method>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.api.reporting.SingleFileReport" name="report"/>
    </method>
    <method name="getDefaultTargetJdk" returnType="org.gradle.api.plugins.quality.TargetJdk">
        <parameter type="org.gradle.api.JavaVersion" name="javaVersion"/>
    </method>
    <property name="defaultTargetJdk" type="org.gradle.api.plugins.quality.TargetJdk"/>
</type>
