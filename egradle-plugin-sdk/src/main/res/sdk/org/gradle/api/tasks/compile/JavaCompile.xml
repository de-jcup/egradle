<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type language="gradle" name="org.gradle.api.tasks.compile.JavaCompile" version="3.0" superType="org.gradle.api.tasks.compile.AbstractCompile" documented="true">
    <description> Compiles Java source files.

 &lt;pre autoTested=''&gt;
     apply plugin: 'java'
     compileJava {
         &lt;em class='comment'&gt;//enable compilation in a separate daemon process&lt;/em&gt;
         options.fork = true

         &lt;em class='comment'&gt;//enable incremental compilation&lt;/em&gt;
         options.incremental = true
     }
 &lt;/pre&gt;

</description>
    <method name="getClassAnalysisCache" returnType="org.gradle.api.internal.tasks.compile.incremental.analyzer.ClassAnalysisCache"/>
    <method name="getDependencyCacheDir" returnType="java.io.File"/>
    <method name="getJarSnapshotCache" returnType="org.gradle.api.internal.tasks.compile.incremental.jar.JarSnapshotCache"/>
    <method name="getLocalClassSetAnalysisStore" returnType="org.gradle.api.internal.tasks.compile.incremental.deps.LocalClassSetAnalysisStore"/>
    <method name="getLocalJarClasspathSnapshotStore" returnType="org.gradle.api.internal.tasks.compile.incremental.jar.LocalJarClasspathSnapshotStore"/>
    <method name="getOptions" returnType="org.gradle.api.tasks.compile.CompileOptions">
        <description> Returns the compilation options.

 &lt;br&gt;&lt;br&gt;&lt;b class='return'&gt;returns:&lt;/b&gt;The compilation options.

</description>
    </method>
    <method name="getToolChain" returnType="org.gradle.jvm.toolchain.JavaToolChain">
        <description> Returns the tool chain that will be used to compile the Java source.

 &lt;br&gt;&lt;br&gt;&lt;b class='return'&gt;returns:&lt;/b&gt;The tool chain.

</description>
    </method>
    <method name="setDependencyCacheDir" returnType="void">
        <parameter type="java.io.File" name="dependencyCacheDir"/>
    </method>
    <method name="setToolChain" returnType="void">
        <description> Sets the tool chain that should be used to compile the Java source.

 &lt;br&gt;&lt;b class='param'&gt;param:&lt;/b&gt;toolChain The tool chain.

</description>
        <parameter type="org.gradle.jvm.toolchain.JavaToolChain" name="toolChain"/>
    </method>
    <property name="classAnalysisCache" type="org.gradle.api.internal.tasks.compile.incremental.analyzer.ClassAnalysisCache"/>
    <property name="dependencyCacheDir" type="java.io.File"/>
    <property name="jarSnapshotCache" type="org.gradle.api.internal.tasks.compile.incremental.jar.JarSnapshotCache"/>
    <property name="localClassSetAnalysisStore" type="org.gradle.api.internal.tasks.compile.incremental.deps.LocalClassSetAnalysisStore"/>
    <property name="localJarClasspathSnapshotStore" type="org.gradle.api.internal.tasks.compile.incremental.jar.LocalJarClasspathSnapshotStore"/>
    <property name="options" type="org.gradle.api.tasks.compile.CompileOptions" documented="true">
        <description> Returns the compilation options.

 &lt;br&gt;&lt;br&gt;&lt;b class='return'&gt;returns:&lt;/b&gt;The compilation options.

</description>
    </property>
    <property name="toolChain" type="org.gradle.jvm.toolchain.JavaToolChain" documented="true">
        <description> Returns the tool chain that will be used to compile the Java source.

 &lt;br&gt;&lt;br&gt;&lt;b class='return'&gt;returns:&lt;/b&gt;The tool chain.

</description>
    </property>
</type>
