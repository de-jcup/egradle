<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type language="gradle" name="org.gradle.jvm.plugins.JvmTestSuiteBasePlugin" version="3.0" superType="org.gradle.model.RuleSource">
    <description> The base plugin that needs to be applied by all plugins which provide testing support
 for the Java software model.

 &lt;br&gt;(since 2.12)

</description>
    <method name="configureRuntimeClasspath" returnType="void">
        <parameter type="org.gradle.jvm.test.internal.JvmTestSuiteBinarySpecInternal" name="testBinary"/>
        <parameter type="org.gradle.internal.service.ServiceRegistry" name="serviceRegistry"/>
        <parameter type="org.gradle.model.internal.manage.schema.ModelSchemaStore" name="modelSchemaStore"/>
    </method>
    <method name="execute" returnType="void">
        <parameter type="org.gradle.api.tasks.testing.Test" name="test"/>
    </method>
</type>
